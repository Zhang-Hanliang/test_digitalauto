{
    "manifestVersion": "v3",
    "name": "testdigitalauto",
    "interfaces": [
        {
            "type": "vehicle-signal-interface",
            "config": {
                "src": "./app/vspec.json",
                "datapoints": {
                    "required": []
                }
            }
        },
        {
            "type": "pubsub",
            "config": {
                "reads": [
                    "sampleapp/getSpeed"
                ],
                "writes": [
                    "sampleapp/getSpeed/response",
                    "sampleapp/currentSpeed"
                ]
            }
        }
    ]
}
